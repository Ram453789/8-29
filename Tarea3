#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <windows.h>

int main ()
{
	system("cls");
	printf("\033[0;31m");
	int a=3;
	if (a==3)
		{
			printf( "     *\n   //|\\\\ \n ////|\\\\\\\\ \n/////|\\\\\\\\\\ \n   IIIII \n");
		}
	else
		if (a==5)
		{
			printf("       *\n     //|\\\\ \n   ////|\\\\\\\\ \n  /////|\\\\\\\\\\ \n //////|\\\\\\\\\\\\ \n///////|\\\\\\\\\\\\\\ \n     IIIII \n ");
		}
		else
		{
			printf("          *\n        //|\\\\ \n      ////|\\\\\\\\ \n     /////|\\\\\\\\\\ \n    //////|\\\\\\\\\\\\ \n   ///////|\\\\\\\\\\\\\\ \n  ////////|\\\\\\\\\\\\\\\\ \n /////////|\\\\\\\\\\\\\\\\\\ \n        IIIII \n ");
		}	
return 0;	
}

